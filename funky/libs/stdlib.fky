module stdlib where

    undefined = error "undefined"
    otherwise = True

    # Identity function
    id x = x

    # Constant function
    const x = lambda _ -> x

    # Boolean negation
    not  True   =  False
    not  False  =  True

    # Flipping function application
    flip f x y = f y x

    # Function composition
    compose f g x = f (g x)
