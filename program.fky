module test where

    newcons Tree = Branch Tree Integer Tree | Leaf Integer
    newcons List = Cons Integer List | Nil

    otherwise = True

    filter f (Cons a b) | f a       = Cons a (filter f b)
                        | otherwise = filter f b
    filter f Nil = Nil

    map f (Cons a b) = Cons (f a) (map f b)
    map f Nil = Nil

    main = map (lambda x -> x * 2) (Cons 1 Nil)
