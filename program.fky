module test where

    newcons List = Cons Integer List | Nil

    otherwise = True

    filter f Nil         = Nil
    filter f (Cons x xs) | f x       = Cons x (filter f xs)
                         | otherwise = filter f xs

    range x y | x == y    = Cons x Nil
              | otherwise = Cons x (range (x + 1) y)

    main = filter (lambda x -> x % 2 == 0) (range 1 10)


