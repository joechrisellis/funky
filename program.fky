module test where

    otherwise = True

    newcons List = Cons Integer List | Nil

    concatenate (Cons x xs) ys = Cons x (xs `concatenate` ys)
    concatenate Nil         ys  = ys
    concatenate xs          Nil = xs
    concatenate Nil         Nil = Nil

    range m n | m == n    = Cons m Nil
              | otherwise = Cons m (range (m + 1) n)

    main = Nil `concatenate` (Cons 1 Nil) # (range 10 20) `concatenate` (range 0 10)
